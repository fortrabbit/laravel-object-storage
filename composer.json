{
    "name": "bmidget/laravel-object-storage",
    "description": "Flysystem adapter for fortrabbit Object Storage",
    "license": "MIT",
    "keywords": [
        "flysystem",
        "laravel",
        "fortrabbit",
        "s3",
        "object-storage"
    ],
    "authors": [
        {
            "name": "Oliver Stark",
            "email": "os@fortrabbit.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "league/flysystem": "^3.0.0",
        "aws/aws-sdk-php": "^3.0.0",
        "league/flysystem-aws-s3-v3": "^3.0.0",
        "illuminate/support": "^9.0",
        "illuminate/filesystem": "^9.0",
        "league/mime-type-detection": "^1.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "fortrabbit\\ObjectStorage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "fortrabbit\\ObjectStorage\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "phpstan": [
            "vendor/bin/phpstan analyse src --level 5"
        ]
    }
}
